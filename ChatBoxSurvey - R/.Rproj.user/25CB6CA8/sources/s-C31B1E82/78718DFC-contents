Divvy2015 <- q1_15 %>% union(q2_15) %>% union(m7_15) %>% union(m8_15) %>% union(m9_15) %>% union(q4_15)
  
Divvy_Complete <- Divvy2015 %>% union(Divvy2016) %>% union(Divvy2017) %>% union(Divvy2018) %>% union(Divvy2019) 

